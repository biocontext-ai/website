{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "quoteProps": "consistent",
  "arrowParens": "always",
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-imports"]
}
